{
  "name": "student-services",
  "version": "0.3.0",
  "description": "Student Services",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "develop": "cross-env NODE_ENV=development npm start",
    "debug:dev": "cross-env NODE_ENV=development npm start --inspect",
    "debug:express": "DEBUG=express:*  npm start",
    "debug:session": "DEBUG=express-session npm start",
    "prettier": "npx prettier --write .",
    "eslint": "npx eslint .",
    "test": "cross-env NODE_ENV=test "
  },
  "keywords": [
    "student",
    "services"
  ],
  "authors": [
    "Mohammad Amanalikhani <md.akhi.ir@gmail.com> (http://akhi.ir/)"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "randomstring": "^1.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/md-akhi/studentServices.git"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  }
}
